    "def serialize(obj, path):\n",
    "    with open(path, 'wb') as f:\n",
    "        pickle.dump(obj, f)\n",
    "        f.close()\n",
    "\n",
    "def deserialize(path):\n",
    "    with open(path, 'rb') as f:\n",
    "        obj = pickle.load(f) # deserialize using load()\n",
    "    return obj"
